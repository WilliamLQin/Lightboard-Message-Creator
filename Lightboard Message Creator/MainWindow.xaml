<Window x:Class="Lightboard_Message_Creator.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:local="clr-namespace:Lightboard_Message_Creator"
        Title="MainWindow" Height="768" Width="1024"
        Background="Black"
        ContentRendered="Window_ContentRendered" ResizeMode="CanMinimize">
    <Grid x:Name="RootGrid">
        <Grid.ColumnDefinitions>
            <ColumnDefinition Width="1024*"/>
        </Grid.ColumnDefinitions>
        <Grid.RowDefinitions>
            <RowDefinition Height="30"/>
            <RowDefinition Height="200"/>
            <RowDefinition Height="90"/>
            <RowDefinition/>
        </Grid.RowDefinitions>

        <!-- Toolbar -->
        <ToolBar ToolBarTray.IsLocked="True" Margin="-5, -1, -13, 0" Padding="5, 0, 0, 0">
            <Menu ToolBar.OverflowMode="Never">
                <MenuItem x:Name="MenuNew" Header="New" FontSize="13" Click="MenuNew_Click"/>
                <MenuItem x:Name="MenuOpen" Header="Open" FontSize="13" Click="MenuOpen_Click"/>
                <MenuItem x:Name="MenuSave" Header="Save" FontSize="13" Click="MenuSave_Click"/>
            </Menu>
        </ToolBar>

        <!-- Boxes for controls -->
        <Rectangle Stroke="#a0a0a0" Height="180" Margin="10" Grid.Row="1" VerticalAlignment="Center"/>
        <Rectangle Stroke="#a0a0a0" Height="80" Margin="10,0,10,10" Grid.Row="2" VerticalAlignment="Center"/>

        <!-- Bottom Box Controls -->
        <Label Content="Preview Controls" HorizontalAlignment="Left" Margin="25,7,0,57" Grid.Row="2" VerticalAlignment="Center" Width="150" Foreground="White" Height="26"/>
        <Button x:Name="PlayPreview" HorizontalAlignment="Left" VerticalAlignment="Top" Width="30" Margin="30,35,0,0" Grid.Row="2" Height="30" Click="PlayPreview_Click">
            <local:AutoGreyableImage Source="images/Play.png"></local:AutoGreyableImage>
        </Button>
        <Button x:Name="PausePreview" HorizontalAlignment="Left" VerticalAlignment="Top" Width="30" Margin="60,35,0,0" Grid.Row="2" Height="30" IsEnabled="False" Click="PausePreview_Click">
            <local:AutoGreyableImage Source="images/Pause.png"></local:AutoGreyableImage>
        </Button>
        <Button x:Name="StopPreview" HorizontalAlignment="Left" VerticalAlignment="Top" Width="30" Margin="90,35,0,0" Grid.Row="2" Height="30" IsEnabled="False" Click="StopPreview_Click">
            <local:AutoGreyableImage Source="images/Stop.png"></local:AutoGreyableImage>
        </Button>

        <Label Content="Display Mode" HorizontalAlignment="Left" Margin="150,7,0,57" Grid.Row="2" VerticalAlignment="Center" Width="150" Foreground="White" Height="26"/>
        <ComboBox x:Name="DisplayModeSelect" SelectedIndex="0" HorizontalAlignment="Left" VerticalAlignment="Top" Width="150" Margin="150,39,0,0" Grid.Row="2" SelectionChanged="DisplayModeSelect_SelectionChanged" Height="22">
            <ComboBoxItem>Left Scroll</ComboBoxItem>
            <ComboBoxItem>Right Scroll</ComboBoxItem>
            <ComboBoxItem>Center Out</ComboBoxItem>
            <ComboBoxItem>Flashing</ComboBoxItem>
            <ComboBoxItem>Roll Up</ComboBoxItem>
            <ComboBoxItem>Roll Down</ComboBoxItem>
            <ComboBoxItem>Steady On</ComboBoxItem>
        </ComboBox>

        <Label Content="Translate" HorizontalAlignment="Left" Margin="325,7,0,57" Grid.Row="2" VerticalAlignment="Center" Width="150" Foreground="White" Height="26"/>
        <Button x:Name="TranslateLeft" HorizontalAlignment="Left" VerticalAlignment="Top" Width="30" Margin="330,35,0,0" Grid.Row="2" Height="30" Click="TranslateLeft_Click">
            <local:AutoGreyableImage Source="images/LeftArrow.png"></local:AutoGreyableImage>
        </Button>
        <Button x:Name="TranslateRight" HorizontalAlignment="Left" VerticalAlignment="Top" Width="30" Margin="360,35,0,0" Grid.Row="2" Height="30" Click="TranslateRight_Click">
            <local:AutoGreyableImage Source="images/RightArrow.png"></local:AutoGreyableImage>
        </Button>
        <Button x:Name="TranslateUp" HorizontalAlignment="Left" VerticalAlignment="Top" Width="30" Margin="390,35,0,0" Grid.Row="2" Height="30" Click="TranslateUp_Click">
            <local:AutoGreyableImage Source="images/UpArrow.png"></local:AutoGreyableImage>
        </Button>
        <Button x:Name="TranslateDown" HorizontalAlignment="Left" VerticalAlignment="Top" Width="30" Margin="420,35,0,0" Grid.Row="2" Height="30" Click="TranslateDown_Click">
            <local:AutoGreyableImage Source="images/DownArrow.png"></local:AutoGreyableImage>
        </Button>

        <Label Content="Message Size" HorizontalAlignment="Left" Margin="480,7,0,57" Grid.Row="2" VerticalAlignment="Center" Width="150" Foreground="White" Height="26"/>
        <Label Content="Width" HorizontalAlignment="Left" Margin="485,36,0,28" Grid.Row="2" VerticalAlignment="Center" Width="115" Foreground="White" Height="26"/>
        <Label Content="Height" HorizontalAlignment="Left" Margin="640,36,0,28" Grid.Row="2" VerticalAlignment="Center" Width="115" Foreground="White" Height="26"/>
        <TextBox x:Name="MessageWidth" PreviewTextInput="NumberValidationTextBox" VerticalContentAlignment="Center" HorizontalAlignment="Left" Height="26" Margin="530,37,0,0" Grid.Row="2" TextWrapping="Wrap" VerticalAlignment="Top" Width="95"/>
        <TextBox x:Name="MessageHeight" PreviewTextInput="NumberValidationTextBox" VerticalContentAlignment="Center" HorizontalAlignment="Left" Height="26" Margin="690,37,0,0" Grid.Row="2" TextWrapping="Wrap" VerticalAlignment="Top" Width="95"/>

        <Button x:Name="SizeConfirm" Content="Confirm" HorizontalAlignment="Left" Margin="810,35,0,0" Grid.Row="2" VerticalAlignment="Top" Width="80" Height="30" Click="SizeConfirm_Click"/>

        <Button x:Name="ClearMessage" Content="Clear" HorizontalAlignment="Left" Margin="905,35,0,0" Grid.Row="2" VerticalAlignment="Top" Width="80" Height="30" Click="ClearMessage_Click"/>


