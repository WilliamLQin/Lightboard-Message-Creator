        <Label Content="Message" HorizontalAlignment="Left" Margin="175,19,0,155" Grid.Row="1" VerticalAlignment="Center" Width="125" Foreground="White" Height="26"/>
        <TextBox x:Name="TextInput" HorizontalAlignment="Left" Height="125" Margin="175,50,0,0" Grid.Row="1" TextWrapping="Wrap" VerticalAlignment="Top" Width="525"/>

